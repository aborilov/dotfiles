! urxvt.font:             -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso10646-1
urxvt.font:             xft:Inconsolata-g\ for\ Powerline:size=13:antialias=true,xft:Consolas:size=13:antialias=true
! urxvt.boldFont:         -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso10646-1
urxvt.geometry:         80x25
urxvt.saveLines:        1000
urxvt.scrollBar:        false
urxvt.tintColor:        white
urxvt.fading:           15
urxvt.fadeColor:        black
urxvt.shading:          75
urxvt.cursorColor:     green
urxvt*iso14755:        false
urxvt.perl-ext-common: default,clipboard
urxvt.keysym.Shift-Control-C: perl:clipboard:copy
urxvt.keysym.Shift-Control-V: perl:clipboard:paste



urxvt*foreground: #f2f2f2
urxvt*background: #101010

! Normal
! color0    black
! color1    red
! color2    green
! color3    yellow
! color4    blue
! color5    purple
! color6    cyan
! color7    white

urxvt*color0:    #6c6c6c
urxvt*color1:    #e9897c
urxvt*color2:    #b6e77d
urxvt*color3:    #ecebbe
urxvt*color4:    #a9cdeb
urxvt*color5:    #ea96eb
urxvt*color6:    #c9caec
urxvt*color7:    #f2f2f2

! Light
! color8    black
! color9    red
! color10    green
! color11    yellow
! color12    blue
! color13    purple
! color14    cyan
! color15    white

urxvt*color8:    #747474
urxvt*color9:    #f99286
urxvt*color10:    #c3f786
urxvt*color11:    #fcfbcc
urxvt*color12:    #b6defb
urxvt*color13:    #fba1fb
